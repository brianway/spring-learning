<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="car" class="com.brianway.learning.spring.ioc.factorybean.Car"
          p:brand="红旗CA72" p:maxSpeed="200" p:price="20000.00"/>

    <bean id="car-factorybean" class="com.brianway.learning.spring.ioc.factorybean.CarFactoryBean"
          p:carInfo="红旗CA72,200,20000.00"/>

</beans>
